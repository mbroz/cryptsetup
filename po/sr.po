msgid "[OPTION...] <device>"
msgstr "[ОПЦИЈА...] <уређај>"

#: src/cryptsetup_reencrypt.c:1357
#, c-format
msgid "Reencryption will change: volume key%s%s%s%s.\n"
msgstr "Поновно шифровање ће изменити: кључ волумена%s%s%s%s.\n"

